{
    "name": "awscdk-appsync-decorators",
    "version": "0.0.0",
    "description": "Decorators for creating appsync apis using aws-cdk",
    "license": "MIT",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "tsup && yarn version patch",
        "build:dev": "tsup && yalc publish",
        "test": "jest --verbose",
        "coverage": "jest --coverage",
        "format": "prettier \"**/*.+(ts|js|json)\" --write",
        "lint": "eslint \"**/*.+(ts|js|json)\"",
        "fix": "yarn format && yarn lint --fix"
    },
    "dependencies": {
        "reflect-metadata": "0.2.2"
    },
    "devDependencies": {
        "@eslint/js": "9.17.0",
        "@faker-js/faker": "9.3.0",
        "@types/jest": "29.5.14",
        "@types/node": "22.10.2",
        "aws-cdk-lib": "2.173.4",
        "awscdk-appsync-utils": "0.0.614",
        "constructs": "10.4.2",
        "eslint": "9.17.0",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "jest": "29.7.0",
        "prettier": "3.4.2",
        "ts-jest": "29.2.5",
        "tsc": "2.0.4",
        "tsup": "8.3.5",
        "tsx": "4.19.2",
        "typescript": "5.7.2",
        "typescript-eslint": "8.18.2",
        "yalc": "1.0.0-pre.53"
    },
    "peerDependencies": {
        "awscdk-appsync-utils": "~0.0.328"
    }
}
