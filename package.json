{
    "name": "awscdk-appsync-decorators",
    "version": "0.0.4",
    "description": "Decorators for creating appsync apis using aws-cdk",
    "license": "MIT",
    "keywords": [
        "aws",
        "cdk",
        "appsync"
    ],
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "tsup",
        "build:dev": "yarn build && yalc publish",
        "build:bump": "yarn build && yarn version --patch",
        "test": "jest --verbose",
        "test:cov": "jest --coverage",
        "format": "prettier \"**/*.+(ts|js|json)\" --write",
        "lint": "eslint \"**/*.+(ts|js|json)\"",
        "fix": "yarn format && yarn lint --fix"
    },
    "dependencies": {
        "reflect-metadata": "0.2.2"
    },
    "devDependencies": {
        "@eslint/js": "9.33.0",
        "@faker-js/faker": "9.9.0",
        "@swc/core": "1.13.3",
        "@types/jest": "30.0.0",
        "@types/node": "24.3.0",
        "aws-cdk-lib": "2.211.0",
        "awscdk-appsync-utils": "0.0.806",
        "constructs": "10.4.2",
        "eslint": "9.33.0",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "jest": "30.0.5",
        "prettier": "3.6.2",
        "ts-jest": "29.4.1",
        "tsc": "2.0.4",
        "tsup": "8.5.0",
        "tsx": "4.20.4",
        "typescript": "5.9.2",
        "typescript-eslint": "8.40.0",
        "yalc": "1.0.0-pre.53"
    },
    "peerDependencies": {
        "awscdk-appsync-utils": "~0.0.806"
    }
}
